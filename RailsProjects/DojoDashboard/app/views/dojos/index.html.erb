<% if flash[:errors] != nil %>
<% flash[:errors].each do |error|%>
<p><%= error %></p>
<% end %>
<% end %>

<% @dojos.each do |dojo|%>
<p><%= dojo.branch%> | <%= dojo.street%> | <%= dojo.city%> | <%= dojo.state%> <a href="/dojos/<%=dojo.id%>">Show</a> | <a href="/dojos/edit/<%=dojo.id%>">Edit</a> | <a href="/dojos/<%=dojo.id%>" data-method="delete">Delete</a></p>
<% end %>
<a href="/dojos/new">Create New Dojo!</a>
