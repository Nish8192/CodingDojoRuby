<h1>DOJO</h1>
<p>Branch Name: <%= @dojo.branch%></p>
<p>Street: <%= @dojo.street%></p>
<p>City: <%= @dojo.city%></p>
<p>State: <%= @dojo.branch%></p>
<a href="/dojos/<%=@dojo.id%>/students/new">Create New Student</a> | <a href="/">Go Back</a> | <a href="/dojos/edit/<%=@dojo.id%>">Edit</a>

<h2>Student Roster</h2>
<% @students.each do |student|%>
<p><%=student.first_name%> <%=student.last_name%> | <%=student.email%> | <a href="/dojos/students/<%=student.id%>">Show</a> | <a href="/dojos/students/<%=student.id%>/edit">Edit</a> | <a href="/dojos/<%=@dojo.id%>/students/<%=student.id%>" data-method="delete">Delete</a> | </p>
<% end %>
